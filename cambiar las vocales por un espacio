#include <stdio.h>
#include <stdlib.h>

int main()
{
char cadena[100];
int i;
printf("Ingrese una cadena\n");
fgets(cadena, sizeof(cadena),stdin);

for (int i = 0; cadena[i] != '\0'; i++) {
char c = cadena[i];

if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u' ||
    c == 'A' || c == 'E' || c == 'I' || c == 'O' || c == 'U') {
    cadena[i] = ' ';





}
}
printf("Su cadena final es %s\n", cadena);
returnÂ 0;
}
